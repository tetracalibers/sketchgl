{
  "name": "sketchgl",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "module": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "module": "./dist/*.cjs",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*"
      ],
      ".": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "prepare": "vite build && tsc",
    "preview": "vite preview",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "lil-gui": "^0.18.2"
  }
}
