{
  "name": "sketchgl",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./math": {
      "import": "./dist/math.js",
      "require": "./dist/math.cjs"
    },
    "./webgl": {
      "import": "./dist/webgl.js",
      "require": "./dist/webgl.cjs"
    },
    "./camera": {
      "import": "./dist/camera.js",
      "require": "./dist/camera.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "lil-gui": "^0.18.2"
  }
}
